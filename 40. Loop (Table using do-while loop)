import java.util.Scanner;
public class TableDoWhile {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.print("Enter the number:");
		int num = sc.nextInt();
		System.out.println(num);
		int ctr=2;
		do
		{
			int res=num*ctr;
			System.out.println(res);
			ctr++;
		}while(ctr<=10);
	}
}
