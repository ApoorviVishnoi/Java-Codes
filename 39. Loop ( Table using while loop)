import java.util.Scanner;
public class TableWhileLoop {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.print("Enter the number:");
		int num = sc.nextInt();
		System.out.println(num);
		int ctr=2;
		while(ctr<=10)
		{
			int res=num*ctr;
			System.out.println(res);
			ctr++;
		}
	}
}
