public class App7 {

	public static void main(String[] args) {
		int[] x=new int[]{10,5,30,190};
		//copy all numbers into a new array in reverse order of type int without loop
		int[] y=new int[x.length];
		int j = x.length-1;
		for(int i=0;i<y.length;i++)
		{
			y[i]=x[j];
			j--;
		}
		System.out.println(y[3]);
	}
}
