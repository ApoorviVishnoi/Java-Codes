import java.util.Scanner;

public class DigitDemo {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter the number:");
		int num=sc.nextInt();
		int rem;
		while(num!=0)
		{
			rem=num%10;
			System.out.println(rem);
			rem=num/10;
		}

	}

}
