import java.util.Scanner;
public class App3 {
	public static void main(String[] args) {
		
	    //Accept the no. from user and accept the frequency/occurrence/repeat of that no. in array
	    //output must be like this : frequency of the no. is 2

	    int[] x= {1,67,45,2,1,1,90,24,123,56,89,80,10};
	    Scanner sc =new Scanner(System.in);

	    System.out.println("Enter the no.:");
	    int y =sc.nextInt();
	    int c=0;
	    for(int i=0;i<x.length;i++)
	    {
	    	if(x[i]==y)
	    		c++;
	    }
	    System.out.println("Frequency of a no. is: "+c);
	}
}
