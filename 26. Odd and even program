public class OddEven {

	public static void main(String[] args) {
		int n = 20;
		if(n%2!=0)
		{
			System.out.println("Invalid");
		}
		else
		{
			if(n>=20)
			{
				System.out.println("Valid");
			}
			else
			{
				if(n>=14)
				{
					System.out.println("invalid");
				}
				else 
				{
					if(n>=6)
					{
						System.out.println("valid");
					}
					else
					{
						if(n>=1)
						{
							System.out.println("invalid");
						}
					}
				}
			}
		}
	}
}

