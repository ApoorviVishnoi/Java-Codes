import java.util.Scanner;
public class Table {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.print("Enter the number:");
		int num = sc.nextInt();
		System.out.println(num);
		for(int ctr=2;ctr<10;ctr++)
		{
			int res=num*ctr;
			System.out.println(res);
		}
	}
}
