public class Condition1
{
	public static void main(String[] args)
	{
		int ch='/';
		int x=80,y=50;
		if(ch=='+')
		{
		  int z1=x+y;
		  System.out.println(z1);
		}

		if(ch=='-')
		{
		  int z2=x-y;
		  System.out.println(z2);
		}

		if(ch=='*')
		{
		  int z3=x*y;
		  System.out.println(z3);
		}

		if(ch=='/')
		{
		  float z4=(float)x/y;
		  System.out.println(z4);
		}
	}
}
