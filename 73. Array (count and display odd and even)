public class App {

	public static void main(String[] args) {
		int[] x= {1,67,45,90,24,123,56,89,80,10,45,13};
		int evencount=0,oddcount=0;
		for(int i=0;i<x.length;i++)
		{
			if(x[i]%2==0)
			{
				evencount++;
			}
			else
			{
				oddcount++;
			}
		}
		System.out.println("Array have "+ evencount+" evens and "+oddcount+" odds");
	}
}
